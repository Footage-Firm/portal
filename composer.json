{
    "name": "footage-firm/portal",
    "description": "Portal allows you to dispatch Laravel events across multiple apps with ease.",
    "keywords": ["laravel", "events"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Hansen",
            "email": "mathias@storyblocks.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.5.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Storyblocks\\Portal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Storyblocks\\Portal\\PortalServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
